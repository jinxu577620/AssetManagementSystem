<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mphigh.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.mphigh.entity.User">
        <id column="uid" property="uid" />
        <result column="uname" property="uname" />
        <result column="cname" property="cname" />
        <result column="department" property="department" />
        <result column="smauthority" property="smauthority" />
        <result column="umauthority" property="umauthority" />
        <result column="amauthority" property="amauthority" />
        <result column="pauthority" property="pauthority" />
        <result column="apauthority" property="apauthority" />
        <result column="tel" property="tel" />
        <result column="ustate" property="ustate" />
        <result column="email" property="email" />
        <result column="password" property="password" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        uid, uname, cname, department, smauthority, umauthority, amauthority, pauthority, apauthority, tel, ustate, email, password
    </sql>

</mapper>
